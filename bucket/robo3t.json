{
    "version": "1.3.1",
    "description": "Cross platform Web debugging proxy",
    "homepage": "https://github.com/alibaba/lightproxy",
    "license": "BSD-3-Clause",
    "url": "https://download.studio3t.com/robomongo/windows/robo3t-1.3.1-windows-x86_64-7419c406.exe",
    "bin": "Robo3t.exe",
    "shortcuts": [
        [
            "Robo3t.exe",
            "Robo3t"
        ]
    ],
    "pre_install": [
        "Expand-7zipArchive -Path \"$dir\\$fname\" -DestinationPath \"$dir\\_tmp\"",
        "Get-ChildItem -Path \"$dir\\_tmp\" -Depth \"2\" -Include \"*.7z\" | Move-Item -Destination \"$dir\\_tmp\"",
        "Expand-7zipArchive -Path \"$dir\\_tmp\\app-64.7z\" -DestinationPath \"$dir\"",
        "Remove-Item -Recurse \"$dir\\_tmp\", \"$dir\\$fname\""
    ]
    
}